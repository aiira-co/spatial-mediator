  "keywords": [
    "middleware",
    "mediator",
    "psr",
    "psr-7",
    "http",
    "http-message",
    "request",
    "response"
  ],
  "homepage": "https://github.com/php-fig/http-message",
  "license": "MIT",
  "authors": [
    {
      "name": "Kofi Owusu-Afriyie",
      "email": "koathecedi@gmail.com",
      "homepage": "https://aiira.co",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.4",
    "psr/http-server-middleware": "^1.0",
    "spatial/psr7": "^4.0.0-RC2"
  },
  "autoload": {
    "psr-4": {
      "Spatial\\Mediator\\": "src/"
